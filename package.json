{
  "name": "@canterbury-air-patrol/leaflet-dialog",
  "version": "1.2.1",
  "description": "Resizable, movable dialog window for Leaflet",
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "esbuild": "^0.27.0",
    "eslint": "^9.13.0",
    "eslint-plugin-react": "^7.37.1",
    "globals": "^17.0.0",
    "http-server": "^14.1.1",
    "leaflet": "~1.9.2",
    "prettier": "^3.3.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "~7.1.0",
    "jquery": "^4.0.0"
  },
  "main": "Leaflet.Dialog.js",
  "scripts": {
    "check": "eslint *.js && prettier -c *.js",
    "build": "esbuild dialog.js --outdir=out/ --loader:.woff2=file --loader:.ttf=file --bundle --minify",
    "start": "http-server -a localhost -p 8000"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/canterbury-air-patrol/Leaflet.Dialog.git"
  },
  "keywords": [
    "javascript",
    "leaflet",
    "dialog",
    "ui",
    "popup",
    "modal",
    "window"
  ],
  "author": "Scott Parlane",
  "readmeFilename": "README.md"
}
